{
  "compilerOptions": {
    "rootDir": ".",
    "target": "esnext",
    "module": "esnext",
    "declaration": true,
    "noEmit": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictBindCallApply": true,
  },
  "include": [
    "*",
  ],
}